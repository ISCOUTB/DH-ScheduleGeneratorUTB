{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Flutter (Chrome)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "args": ["-d", "chrome", "--web-port", "5173"]
    },

    {
      "name": "Flutter (Edge)",
      "request": "launch",
      "type": "dart",
      "args": ["-d", "edge", "--web-port", "5173"]
    },
    {
      "name": "DH-ScheduleGeneratorUTB (web)",
      "request": "launch",
      "type": "dart",
      "args": [
        "-d",
        "web-server",
        "--web-port",
        "5173",
        "--web-hostname",
        "localhost"
      ]
    },

    {
      "name": "Abrir Chrome (en localhost:5173)",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:5173",
      "webRoot": "${workspaceFolder}"
    },

    {
      "name": "DH-ScheduleGeneratorUTB",
      "request": "launch",
      "type": "dart"
    },
    {
      "name": "DH-ScheduleGeneratorUTB (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "DH-ScheduleGeneratorUTB (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    }
  ]
}
